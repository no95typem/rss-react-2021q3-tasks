{
  "compilerOptions": {
    "module": "ESNext",
    "target": "es5",
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "rootDir": "./",
    "outDir": "./dist",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "downlevelIteration": true,
    "noEmit": false
  },
  "include": ["src", "__mocks__"],
  "exclude": ["configs", "dist", "coverage"]
}
